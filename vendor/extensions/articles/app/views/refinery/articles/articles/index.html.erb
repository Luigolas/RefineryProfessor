<% content_for :body do %>
  <ul class="list-group" id="articles">
    <% @articles.each do |article| %>
      <li class="list-group-item">
        <%= link_to article.title, refinery.articles_article_path(article) %>
      </li>
    <% end %>
  </ul>
<% end %>

<%= render '/refinery/content_page' %>
